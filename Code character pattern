#include<iostream>
using namespace std;


int main(){
    int n;
    cin>>n;
    char a = 'A';
    for (int i = 1; i <=n; i++){
      char starchar = 'A' + i - 1;
      for(int j = 1; j <= i; j++){
          char a = starchar + j - 1;
            cout << a;

        }
        cout << endl;

    }

}
