void bubbleSort(int *input, int size)
{
    //int temp =0;
    for(int j = 0 ;j<size-1;j++){ //added
    for(int i =0; i<size - 1 - j; i++){ //correction in condition
        if(input[i]>input[i+1]){
            int temp = input[i];
            input[i] = input[i+1];
            input[i+1]= temp;
        }
        // else {
        //     continue;
        }
    }
}
