#include <iostream>
using namespace std;

void print2DArray(int **input, int row, int col) {
	// Write your code here
   int k = row;
    for(int i = 0 ; i < row; i++){
        for(int j = k-1 ; j >= 0 ; j--){
            for(int l = 0 ; l < col ; l++){
                cout << input[i][l] << " ";
            }
            cout << endl;
        }
        k--;
    }
}
