int arrayRotateCheck(int *input, int size)
{
    int i = 0;
    int count = 1;
    if (size == 0){
        return 0;
    }
    while(i < size){
        
        
        if (input [i] < input[i+1]){;
              count++;
              i++;}
        else {
            break;
        }
        if (count== size){
            return 0;
        }
        
    }
    return count;
}
