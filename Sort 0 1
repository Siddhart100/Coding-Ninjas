void sortZeroesAndOne(int *input, int size)
{
    int i=0;
    int j=size-1;
    for(i=0;i<j;i++)
    {
        if(input[i]==1)
            {
                for(;j>i;j--)
                {
                    if(input[j]==0)
                    {
                        input[i]=0;
                        input[j]=1;
                        break;
                    }
                    
                }
                
            }
        
    }
    
}
